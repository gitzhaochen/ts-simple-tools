{
  "name": "@zg/tools",
  "version": "1.0.3",
  "description": "基建工具一些通用处理函数等",
  "module": "dist/esm/index.js",
  "main": "dist/umd/index.js",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -c scripts/rollup.config.umd.js -w",
    "build": "npm run build:esm & npm run build:umd",
    "build:esm": "rm -rf dist/esm/ && cross-env NODE_ENV=production rollup -c scripts/rollup.config.esm.js",
    "build:umd": "rm -rf dist/umd/ && cross-env NODE_ENV=production rollup -c scripts/rollup.config.umd.js",
    "release": "node scripts/release.js"
  },
  "author": "allenzhaochen",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/runtime": "^7.8.4",
    "@babel/runtime-corejs3": "^7.8.4",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-image": "^2.0.4",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-url": "^4.0.2",
    "autoprefixer": "^9.7.4",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.0",
    "release-easy": "^1.2.1",
    "rollup": "^1.32.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "core-js": "^3.6.4"
  }
}
