{
  "name": "@zg/tools",
  "version": "1.0.5",
  "description": "基建工具一些通用处理函数等",
  "module": "dist/esm/index.js",
  "main": "dist/umd/index.js",
  "sideEffects": false,
  "files": [
    "dist",
    "types/*.d.ts"
  ],
  "scripts": {
    "lint": "eslint --cache --ext .js,.ts --ignore-path .eslintignore .",
    "lintfix": "eslint --fix --ext .js,.ts --ignore-path .eslintignore .",
    "dev": "cross-env NODE_ENV=development rollup -c scripts/rollup.config.js -w",
    "build": "rm -rf dist/ && cross-env NODE_ENV=production rollup -c scripts/rollup.config.js",
    "release": "npm run build && standard-version --release-as patch && git push --follow-tags && npm publish",
    "test": ""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "allenzhaochen",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.8.4",
    "@babel/runtime-corejs3": "^7.8.4",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "autoprefixer": "^9.7.4",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "number-precision": "^1.3.2",
    "prettier": "^2.0.5",
    "rollup": "^2.9.1",
    "rollup-plugin-terser": "^5.2.0",
    "standard-version": "^8.0.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "core-js": "^3.6.4",
    "@babel/runtime": "^7.8.4",
    "number-precision": "^1.3.2"
  }
}
